{
  "name": "pt-manager",
  "description": "Another PTPP which build with userscript",
  "version": "0.0.1",
  "author": {
    "name": "Rhilip",
    "email": "rhilipruan@gmail.com"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "lint": "eslint src/**/*.{js,ts}",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags",
    "anylize": "npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.js",
    "dev": "webpack --mode development --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rhilip/PT-Manager"
  },
  "private": true,
  "dependencies": {
    "axios": "0.19.2",
    "axios-userscript-adapter": "0.0.4",
    "element-ui": "2.13.2",
    "jquery": "3.5.1",
    "replace-css-url": "^1.2.6",
    "vue": "2.6.11",
    "vue-router": "3.3.4"
  },
  "devDependencies": {
    "@types/tampermonkey": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "3.6.0",
    "eslint": "7.2.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "6.1.1",
    "style-loader": "1.2.1",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "userscript-metadata-webpack-plugin": "0.0.6",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-livereload-plugin": "2.3.0",
    "webpack-merge": "4.2.2"
  }
}
